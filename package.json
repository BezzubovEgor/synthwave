{
  "name": "synthwave",
  "version": "0.0.1",
  "description": "new music service",
  "repository": "https://github.com/BezzubovEgor/synthwave.git",
  "author": "ybezzubau",
  "license": "MIT",
  "private": false,
  "scripts": {
    "start": "run-p start:**",
    "start:services": "cd services && npm start",
    "start:admin": "cd admin && cross-env PORT=3001 npm start",
    "build": "run-s build:** copy:admin",
    "build:services": "cd services && npm run build",
    "build:admin": "cd admin && cross-env PUBLIC_URL=/admin npm run build",
    "copy:admin": "cpx admin/build/**/* services/dist/assets/admin -C",
    "serve": "cd services && npm run serve",
    "preinstall": "run-s install:**",
    "install:services": "cd services && npm i",
    "install:admin": "cd admin && npm i",
    "heroku-prebuild": "cd services && npm i --only=dev"
  },
  "engines": {
    "node": "10.16.1"
  },
  "devDependencies": {
    "cpx": "^1.5.0",
    "cross-env": "^5.2.0",
    "npm-run-all": "^4.1.5"
  }
}
